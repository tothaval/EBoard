<Application x:Class="EBoard.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EBoard"
             xmlns:eboardSDK="clr-namespace:EBoardSDK.Models;assembly=EBoardSDK">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/EBoardSDK;component/Styles/Button.xaml"/>
                <ResourceDictionary Source="/EBoardSDK;component/Styles/Menu.xaml"/>
                <ResourceDictionary Source="/EBoardSDK;component/Utilities/DataTemplate.xaml"/>
                <ResourceDictionary Source="/EBoardSDK;component/Plugins/PluginManifest.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
