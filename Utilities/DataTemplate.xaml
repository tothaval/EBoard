<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:eboard="clr-namespace:EBoard"
                    xmlns:vms="clr-namespace:EBoard.ViewModels"
                    xmlns:views="clr-namespace:EBoard.Views">

    <DataTemplate DataType="{x:Type vms:MainViewModel}">
        <eboard:MainWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:MainWindowMenuBarViewModel}">
        <views:MainWindowMenuBar/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:MainWindowLogoutBarViewModel}">
        <views:MainWindowLogoutBar/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:EBoardBrowserViewModel}">
        <views:EBoardBrowserView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:EBoardViewModel}">
        <views:EBoardView/>
    </DataTemplate>

    <!--<DataTemplate DataType="{x:Type cp_vms:EButtonViewModel}">
        <cp_views:EButton/>
    </DataTemplate>-->


</ResourceDictionary>